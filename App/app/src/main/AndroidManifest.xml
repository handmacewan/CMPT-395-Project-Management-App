<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication"
        tools:targetApi="31">
        <activity
            android:name=".EditAssignment"
            android:exported="false" />
        <activity
            android:name=".Weekly"
            android:label="Schedule"
            android:exported="false"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EmployeeInfo"
            android:label="Employee Information"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ViewEmployee"
            android:label="View Employee"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".EditEmployee"
            android:label="Edit Employee"
            android:parentActivityName=".ViewEmployee" />
        <activity
            android:name=".ShiftAssignment"
            android:label="Shift Assignment"
            android:parentActivityName=".Weekly" />
    </application>

</manifest>